{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@api": ["api"],
      "@api/*": ["api/*"],
      "@assets": ["assets"],
      "@assets/*": ["assets/*"],
      "@base": ["components/base"],
      "@base/*": ["components/base/*"],
      "@components": ["components"],
      "@components/*": ["components/*"],
      "@compound": ["./components/compound"],
      "@compound/*": ["./components/compound/*"],
      "@data": ["data"],
      "@data/*": ["data/*"],
      "@constants": ["data/constants"],
      "@constants/*": ["data/constants/*"],
      "@models": ["data/models"],
      "@hooks": ["hooks"],
      "@hooks/*": ["hooks/*"],
      "@views": ["views"],
      "@views/*": ["views/*"],
      "@styles": ["styles"],
      "@styles/*": ["styles/*"],
      "@utils": ["utils"],
      "@utils/*": ["utils/*"],
      "@libs": ["utils/libs"],
      "@libs/*": ["utils/libs/*"]
    }
  }
}
